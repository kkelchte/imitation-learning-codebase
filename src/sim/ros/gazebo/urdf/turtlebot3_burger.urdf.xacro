  <!-- Wide angle camera -->
  <xacro:include filename="wide_angle_camera.urdf.xacro" />
  <xacro:wide_angle_camera name="camera" parent="base_link">
  <origin xyz="-0.017 0.0 0.24"/>
  </xacro:wide_angle_camera>
</robot>